{
  "name": "dbgate-plugin-duckdblite",
  "main": "dist/backend.js",
  "version": "1.1.3-alpha.1",
  "homepage": "https://dbgate.org",
  "description": "duckdblite connect plugin for DbGate",
  "repository": {
    "type": "git",
    "url": "https://github.com/dbgate/dbgate"
  },
  "author": "cool navy",
  "license": "GPL-3.0",
  "keywords": [
    "dbgate",
    "dbgateplugin"
  ],
  "files": [
    "dist",
    "icon.svg"
  ],
  "scripts": {
    "build:frontend": "webpack --config webpack-frontend.config",
    "build:frontend:watch": "webpack --watch --config webpack-frontend.config",
    "build:backend": "webpack --config webpack-backend.config.js",
    "build": "yarn build:frontend && yarn build:backend",
    "plugin": "yarn build && yarn pack && dbgate-plugin dbgate-plugin-duckdblite",
    "plugout": "dbgate-plugout dbgate-plugin-duckdblite",
    "copydist": "yarn build && yarn pack && dbgate-copydist ../dist/dbgate-plugin-duckdblite",
    "prepublishOnly": "yarn build"
  },
  "devDependencies": {
    "dbgate-tools": "^6.0.0-alpha.1",
    "dbgate-plugin-tools": "^1.0.8",
    "byline": "^5.0.0",
    "webpack": "^5.91.0",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "lodash": "^4.17.21",
    "dbgate-query-splitter": "^4.11.3"
  },
  "optionalDependencies": {
    "duckdb-async": "1.1.3"
  }
}
